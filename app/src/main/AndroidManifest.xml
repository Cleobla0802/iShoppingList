<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.iescarrillo.ishoppinglist">

    <application
        android:allowBackup="true"
        android:label="iShoppingList"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity android:name=".AnadirPendienteActivity"></activity>
        <activity android:name=".AnadirActivity"></activity>
        <activity android:name=".EditarActivity"></activity>
        <activity android:name=".DetalleActivity"></activity>

        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

    </application>

</manifest>
